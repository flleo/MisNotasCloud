<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/ListaFragment">

    <fragment
        android:id="@+id/ListaFragment"
        android:name="utilidades.misnotas.fragment.ListaFragment"
        tools:layout="@layout/fragment_lista">

        <action
            android:id="@+id/action_ListaFragment_to_NotaFragment"
            app:destination="@id/NotaFragment" />
    </fragment>
    <fragment
        android:id="@+id/NotaFragment"
        android:name="utilidades.misnotas.fragment.NotaFragment"
        android:label="@string/nota_fragment_label"
        tools:layout="@layout/fragment_nota">

        <action
            android:id="@+id/action_NotaFragment_to_ListaFragment"
            app:destination="@id/ListaFragment" />
    </fragment>
</navigation>
